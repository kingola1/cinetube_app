{
	"expo": {
		"name": "cinetube",
		"slug": "cinetube",
		"orientation": "portrait",
		"icon": "./assets/icon.png",
		"userInterfaceStyle": "dark",
		"splash": {
			"image": "./assets/spinner.png",
			"resizeMode": "contain",
			"backgroundColor": "#000000"
		},
		"updates": {
			"fallbackToCacheTimeout": 0
		},
		"assetBundlePatterns": ["**/*"],
		"ios": {
			"supportsTablet": true
		},
		"android": {
			"adaptiveIcon": {
				"foregroundImage": "./assets/adaptive-icon.png",
				"backgroundColor": "#000000"
			}
		},
		"web": {
			"favicon": "./assets/favicon.png"
		},
		"plugins": [
			[
				"expo-camera",
				{
					"cameraPermission": "Allow Cinetube to access your camera."
				}
			],
			[
				"expo-media-library",
				{
					"photosPermission": "Allow Cinetube to access your photos.",
					"savePhotosPermission": "Allow Cinetube to save photos.",
					"isAccessMediaLocationEnabled": true
				}
			]
		]
	}
}
